{
  "name": "dlds/yii2-mlm",
  "description": "Yii2 Multi Level Marketing component",
  "keywords": [
    "yii2",
    "mlm",
    "component"
  ],
  "type": "yii2-extension",
  "license": "BSD",
  "support": {
    "issues": "https://github.com/dlds/yii2-mlm/issues",
    "wiki": "https://github.com/dlds/yii2-mlm/wiki",
    "source": "https://github.com/dlds/yii2-mlm"
  },
  "authors": [
    {
      "name": "Jiri Svoboda",
      "email": "jiri.svoboda@dlds.cz"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.12",
    "dlds/yii2-nested-sets": "~1.3.1"
  },
  "require-dev": {
    "codeception/codeception": "^2.3.3",
    "codeception/specify": "^0.4.6",
    "codeception/verify": "^0.3.3"
  },
  "autoload": {
    "psr-4": {
      "dlds\\mlm\\": "src",
      "dlds\\mlm\\app\\": "app",
      "dlds\\mlm\\tests\\": "tests"
    }
  }
